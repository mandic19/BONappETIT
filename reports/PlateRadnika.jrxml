	<queryString language="SQL">
		<![CDATA[SELECT r."RAD_OZNAKA",
	r."RAD_IME",
	r."RAD_PREZIME",
	rm."RM_NAZIV",
	r."RAD_PLATA"
FROM "RADNIK" r
	join "RADNO_MJESTO" rm ON 
	 rm."RM_OZNAKA" = r."RM_OZNAKA" 
WHERE 
	 r."PS_JIB" = $P{JIB} 
	 AND r."OJ_OZNAKA" = $P{OrgJed} 
	 AND r."DR_OZNAKA" = $P{DrzOzn} 
	 AND r."TPS_OZNAKA" = $P{TipPosSis} 
ORDER BY r."RAD_PLATA" DESC]]>
	</queryString>
